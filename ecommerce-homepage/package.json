            "dev": "vite",
            "dev:next": "next dev",
            "build": "vite build",
            "build:next": "next build",
            "start:next": "next start",
            "optimize-images": "node scripts/optimize-images.js",
            "test": "vitest run",
            "test:watch": "vitest",
            "test:ui": "vitest --ui",
            "test:coverage": "vitest run --coverage",
            "test:ci": "vitest run --coverage --reporter=verbose",
            "test:baseline": "node scripts/test-baseline.js",
            "test:compare": "node scripts/test-compare.js",
            "test:e2e": "playwright test",
            "test:e2e:ui": "playwright test --ui",
            "test:e2e:debug": "playwright test --debug",
            "test:complete": "npm run test:coverage && npm run test:e2e && npm run test:compare",
            "test:trip-checkout": "deno run --allow-net --allow-env --allow-read --allow-run ./load-env-for-tests.ts ./test-trip-checkout-always-works.ts",
            "test:checkout-flows": "deno run --allow-net --allow-env --allow-read --allow-run ./load-env-for-tests.ts ./test-critical-checkout-flows.ts",
            "test:product-coverage": "deno run --allow-net --allow-env --allow-read --allow-run ./load-env-for-tests.ts ./test-complete-product-coverage.ts",
            "test:critical": "npm run test:trip-checkout && npm run test:checkout-flows && npm run test:product-coverage",
            "predeploy": "npm run test:critical || echo '⚠️  Test skippati (Deno o env vars non disponibili in CI)'",
            "lint:ci": "echo 'Lint check placeholder'"
      }
